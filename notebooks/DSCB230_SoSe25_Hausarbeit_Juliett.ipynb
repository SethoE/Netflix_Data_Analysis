    "    \"import plotly.express as px\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Pivot: Zeilen=Jahr, Spalten=Genre\\n\",\n",
    "    \"df_pivot = df_grouped.pivot(index=\\\"release_date\\\", columns=\\\"genre_name\\\", values=\\\"avg_rating\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"fig = px.imshow(\\n\",\n",
    "    \"    df_pivot.T,\\n\",\n",
    "    \"    labels=dict(x=\\\"Jahr\\\", y=\\\"Genre\\\", color=\\\"Durchschnittliche Bewertung\\\"),\\n\",\n",
    "    \"    x=df_pivot.index,\\n\",\n",
    "    \"    y=df_pivot.columns,\\n\",\n",
    "    \"    color_continuous_scale=\\\"Inferno\\\",  # sehr kontraststark\\n\",\n",
    "    \"    zmin=6,  # minimale Bewertung (blau/dunkel)\\n\",\n",
    "    \"    zmax=8,  # maximale Bewertung (gelb/hell)\\n\",\n",
    "    \"    title=\\\"Heatmap der Durchschnittlichen Bewertungen pro Genre und Jahr\\\"\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"fig.update_traces(\\n\",\n",
    "    \"    hovertemplate=\\\"<b>Genre:</b> %{y}<br><b>Jahr:</b> %{x}<br><b>Bewertung:</b> %{z:.2f}<extra></extra>\\\"\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"fig.show()\\n\"\n",
    "   ]\n",
