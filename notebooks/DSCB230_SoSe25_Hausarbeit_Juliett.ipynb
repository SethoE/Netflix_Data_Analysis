    "    \"import json\\n\",\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"PATH_TO_ALL_EUROPEAN_MOVIES = \\\"./data/raw/fragestellung_2/2025-05-18_version1_all_european_movies.json\\\"\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"with open(PATH_TO_ALL_EUROPEAN_MOVIES, \\\"r\\\") as file:\\n\",\n",
    "    \"    all_movies = json.load(file)\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 2) DataFrame bauen\\n\",\n",
    "    \"df = pd.DataFrame(all_movies)\\n\",\n",
    "    \"df[\\\"release_date\\\"] = pd.to_datetime(df[\\\"release_date\\\"]).dt.year\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 4) Jeden Film pro Genre aufsplitten (\\\"explode\\\")\\n\",\n",
    "    \"df = df.explode(\\\"genre_ids\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 6) Gruppieren nach (year, genre_id) und Durchschnitt berechnen\\n\",\n",
    "    \"df_grouped = (\\n\",\n",
    "    \"    df\\n\",\n",
    "    \"    .groupby([\\\"release_date\\\", \\\"genre_ids\\\"])[\\\"vote_average\\\"]\\n\",\n",
    "    \"    .mean()\\n\",\n",
    "    \"    .reset_index(name=\\\"avg_rating\\\")\\n\",\n",
    "    \")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# 7) Optional: Genre-ID → Genre-Name mappen, falls du ein Mapping hast:\\n\",\n",
