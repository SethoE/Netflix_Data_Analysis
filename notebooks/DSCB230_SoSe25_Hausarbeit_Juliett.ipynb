    "   \"source\": [\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"from config.config import TMDB_BEARER_TOKEN, TMDB_API_KEY\\n\",\n",
    "    \"from tmdb.discover import Discover\\n\",\n",
